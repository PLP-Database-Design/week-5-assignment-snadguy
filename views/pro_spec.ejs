<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Providers List</title>
</head>
<body>
    <h1>Providers List</h1>
    <table border="1">
        <thead>
            <tr>
                <th>Provider Id</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Specialty</th>
                <th>Email Address</th>
                <th>Phone Number</th>
                <th>Date Joined</th>
            </tr>
        </thead>
        <tbody>
            <% results.forEach(provider => { %>
                <tr>
                    <td><%= provider.provider_id %></td>
                    <td><%= provider.first_name %></td>
                    <td><%= provider.last_name %></td>
                    <td><%= provider.provider_specialty %></td>
                    <td><%= provider.email_address %></td>
                    <td><%= provider.phone_number %></td>
                    <td><%= new Date(provider.date_joined).toLocaleDateString('en-GB') %></td>
                    
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
